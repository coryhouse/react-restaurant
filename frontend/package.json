{
  "name": "react-restaurant",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite --port 3000 --strictPort",
    "build": "bun run typecheck && vite build",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "preview:app": "vite preview",
    "pw": "playwright test",
    "pw-debug": "playwright test --debug",
    "generate": "node src/scripts/generateFakeData.ts",
    "format": "biome format --write .",
    "check-format": "biome format .",
    "check": "concurrently 'bun lint' 'bun run build' 'bun pw' 'bun cspell'",
    "typecheck": "tsc --noEmit --project tsconfig.app.json",
    "prepare": "husky",
    "cspell": "cspell . --no-progress --show-suggestions --gitignore"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,css,md}": [
      "biome check --write --no-errors-on-unmatched --files-ignore-unknown=true"
    ]
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.12",
    "@tanstack/react-query": "^5.85.5",
    "@tanstack/react-query-devtools": "^5.85.5",
    "@tanstack/react-router": "^1.5.0",
    "@tanstack/router-devtools": "^1.5.0",
    "@tanstack/router-plugin": "^1.131.27",
    "concurrently": "^9.2.0",
    "cspell": "^9.6.0",
    "json-server": "^0.17.0",
    "ky": "^1.9.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-error-boundary": "^6.0.0",
    "sonner": "^2.0.7",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.14",
    "@faker-js/faker": "^10.0.0",
    "@playwright/test": "^1.55.0",
    "@tanstack/router-vite-plugin": "^1.5.0",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.1",
    "babel-plugin-react-compiler": "^19.1.0-rc.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "tailwindcss": "^4.1.12",
    "typescript": "^5.9.2",
    "vite": "^7.1.3"
  }
}
