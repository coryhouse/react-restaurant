{
  "name": "react-restaurant",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "concurrently npm:start:app npm:start:api",
    "start:app": "vite --port 3000 --strictPort",
    "start:api": "json-server --port 3001 db.json --delay 1500",
    "build": "tsc && vite build",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "preview:app": "vite preview",
    "preview": "concurrently npm:preview:app npm:start:api",
    "pw": "playwright test",
    "generate": "node src/scripts/generateFakeData.ts"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.12",
    "@tanstack/query-core": "^5.85.5",
    "@tanstack/query-db-collection": "^0.2.5",
    "@tanstack/react-db": "^0.1.6",
    "@tanstack/react-router": "^1.5.0",
    "@tanstack/router-devtools": "^1.5.0",
    "@tanstack/router-plugin": "^1.131.27",
    "@types/react-window": "^1.8.8",
    "concurrently": "^9.2.0",
    "json-server": "^0.17.0",
    "ky": "^1.9.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-error-boundary": "^6.0.0",
    "react-window": "^1.8.11",
    "sonner": "^2.0.7",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.0.0",
    "@playwright/test": "^1.55.0",
    "@tanstack/router-vite-plugin": "^1.5.0",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.1",
    "babel-plugin-react-compiler": "^19.1.0-rc.2",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^6.0.0-rc.1",
    "eslint-plugin-react-refresh": "^0.4.5",
    "globals": "^16.3.0",
    "tailwindcss": "^4.1.12",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.40.0",
    "vite": "^7.1.3"
  }
}
